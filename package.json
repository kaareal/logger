{
  "name": "@kaareal/logger",
  "version": "1.0.1",
  "description": "Logger for node",
  "keywords": [
    "config",
    "env",
    "environment",
    "configuration",
    "env variables"
  ],
  "main": "./lib/index.js",
  "typings": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaareal/config"
  },
  "author": "Kaare Larsen",
  "license": "MIT",
  "engines": {
    "node": ">=10.*"
  },
  "scripts": {
    "cleanup": "rm -rf lib && rm -rf types",
    "prebuild": "npm run cleanup",
    "build": "tsc",
    "test": "jest",
    "prerelease": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@types/node": "10.*",
    "@types/prettier": "1.19.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "24.3.0",
    "tslib": "1.10.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "printWidth": 120
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)"
    ]
  }
}
